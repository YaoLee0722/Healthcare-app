<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.careapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CareApp">
        <activity android:name=".AchieveActivity"></activity>
        <activity android:name=".NoteActivity" />
        <activity android:name=".NewNoticeActivity" />
        <activity android:name=".AdviceActivity" />
        <activity android:name=".PlanActivity" />
        <activity android:name=".MonitorActivity" />
        <activity android:name=".DialogActivity" />
        <activity android:name=".ArchivesActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>